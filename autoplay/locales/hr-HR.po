#: autoplay/autoplay.py:17
#, docstring
msgid "Automatically play music that a user is listening to on Spotify."
msgstr "Automatski reproduciraj glazbu koju neki korisnik sluša na Spotifyu."

#: autoplay/autoplay.py:28
#, docstring
msgid "Set the member to track for autoplay."
msgstr "Postavite člana kojeg želite pratiti za automatsku reprodukciju."

#: autoplay/autoplay.py:30
msgid "PyLavPlayer is not loaded."
msgstr "PyLavPlayer nije učitan."

#: autoplay/autoplay.py:34
msgid "No longer tracking any member."
msgstr "Ne pratim više ikoga."

#: autoplay/autoplay.py:38
msgid "I'll now play whatever {member} is listening to."
msgstr "Sada ću puštat što god {member} sluša."

